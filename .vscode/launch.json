  "version": "0.2.0",
  "configurations": [
    {
      "name": "aquatrackingapp",
      "request": "launch",
      "type": "dart"
    },
    {
      "name": "aquatrackingapp ( localhost )",
      "request": "launch",
      "type": "dart",
      "args": [
        "--dart-define",
        "DEBUG=true",
        "--dart-define",
        "API_BASE_URL=http://localhost:3000/"
      ]
    },
    {
      "name": "aquatrackingapp ( integration )",
      "request": "launch",
      "type": "dart",
      "args": [
        "--dart-define",
        "DEBUG=true",
        "--dart-define",
        "API_BASE_URL=https://integration.aquatracking.fr/api/"
      ]
    },
    {
      "name": "aquatrackingapp (profile mode)",
      "request": "launch",
      "type": "dart",
      "flutterMode": "profile"
    },
    {
      "name": "aquatrackingapp (release mode)",
      "request": "launch",
      "type": "dart",
      "flutterMode": "release"
    }
  ]
}
